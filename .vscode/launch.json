            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
        "name": "Attach to LangGraph",
        "type": "debugpy",
        "request": "attach",
        "connect": {
                "host": "0.0.0.0",
                "port": 5678
            },
        }
}