        {
            "name": "debug FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": ["app:app","--reload"],
            "cwd": "${workspaceFolder}\\courses\\ML\\linear-regression\\kaggle-linear-regression\\model_as_rest"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
